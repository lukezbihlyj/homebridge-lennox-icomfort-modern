    "icomfort",
    "s30",
    "s40",
    "e30",
    "m30",
    "wifi",
    "thermostat",
    "hvac",
    "climate",
    "homekit"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.1",
    "homebridge": "^1.6.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "typescript": "^5.0.0"
  }
}
